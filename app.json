{
  "expo": {
    "name": "Plataforma Jornada",
    "slug": "Plataforma_Jornada",
    "version": "1.0.5",
    "orientation": "default",
    "platforms": [
      "android",
      "ios"
    ],
    "android": {
      "package": "com.ibacbrasil.jornada",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "WAKE_LOCK",
        "MODIFY_AUDIO_SETTINGS",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.ibacbrasil.jornada",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleDevelopmentRegion": "pt",
        "CFBundleLocalizations": [
          "pt"
        ],
        "NSCameraUsageDescription": "Precisamos acessar a câmera para que você possa usar funcionalidades do site dentro do app.",
        "NSMicrophoneUsageDescription": "Precisamos do microfone para chamadas e gravações dentro do app.",
        "NSPhotoLibraryUsageDescription": "Precisamos acessar suas fotos para carregar e baixar arquivos quando solicitado.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "icon": "assets/images/icon.png",
    "splash": {
      "image": "assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "extra": {
      "eas": {
        "projectId": "48d2d7f3-6f3e-47c8-b489-a1334ff8717e"
      }
    },
    "plugins": [
      "expo-audio",
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "proguardRules": "proguard-rules.pro"
          }
        }
      ]
    ]
  }
}
